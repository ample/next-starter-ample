{
  "name": "site",
  "private": true,
  "description": "Ample Next.js Starter",
  "version": "0.1.0",
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "classnames": "^2.3.1",
    "next": "^12.0.2",
    "postcss": "^8.3.11",
    "postcss-em-media-query": "^4.1.0",
    "postcss-loader": "^6.2.0",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^6.0.0",
    "postcss-sort-media-queries": "^4.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rfs": "^9.0.6",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-jest": "^6.3.12",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/preset-scss": "file:local_modules/@storybook/preset-scss",
    "@storybook/react": "^6.3.12",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint": "^8.1.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "prettier-stylelint": "^0.4.2",
    "react-test-renderer": "^17.0.2",
    "storybook-dark-mode": "^1.0.8",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^2.0.0",
    "stylelint-order": "^5.0.0"
  },
  "keywords": [
    "ample",
    "next.js"
  ],
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "scripts": {
    "build": "DEBUG_MEDIA_QUERIES=false next build",
    "develop": "DEBUG_MEDIA_QUERIES=true next dev",
    "lint": "eslint -c .eslintrc.js ./src && stylelint --config stylelint.config.js './src/**/*.scss'",
    "postinstall": "husky install",
    "start": "next start",
    "storybook": "BROWSER=none start-storybook --modern -s ./public -p 6006 --quiet",
    "storybook:build": "yarn run test:generate-output && build-storybook -c .storybook -o build/",
    "test": "jest ./src",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test:update": "jest ./src --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ample/next-starter-ample"
  },
  "bugs": {
    "url": "https://github.com/ample/next-starter-ample/issues"
  }
}
