{
  "name": "ample-next-starter",
  "description": "Ample Next.js Starter",
  "keywords": [
    "ample",
    "next.js"
  ],
  "license": "MIT",
  "private": true,
  "version": "0.1.0",
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "^12.1.0",
    "postcss": "^8.4.12",
    "postcss-em-media-query": "^4.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.4.3",
    "postcss-pxtorem": "^6.0.0",
    "postcss-sort-media-queries": "^4.2.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rfs": "^9.0.6",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-jest": "^6.3.12",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/preset-scss": "file:local_modules/@storybook/preset-scss",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.15.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.1.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.6.0",
    "prettier-stylelint": "^0.4.2",
    "react-test-renderer": "^17.0.2",
    "storybook-dark-mode": "^1.0.8",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0"
  },
  "scripts": {
    "build": "DEBUG_MEDIA_QUERIES=false next build",
    "clean": "rm -rf .next",
    "develop": "DEBUG_MEDIA_QUERIES=true next dev",
    "lint": "eslint -c .eslintrc.js . && stylelint --config stylelint.config.js './**/*.scss'",
    "start": "next start",
    "storybook": "yarn run test:generate-output && BROWSER=none start-storybook --modern -s ./public -p 6006 --quiet",
    "storybook:build": "yarn run test:generate-output && build-storybook -c .storybook -o build/",
    "test": "jest .",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test:update": "jest . --updateSnapshot"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ample/next-starter-ample"
  },
  "bugs": {
    "url": "https://github.com/ample/next-starter-ample/issues"
  },
  "volta": {
    "node": "14.19.1",
    "yarn": "1.22.18"
  }
}
