{
  "name": "site",
  "private": true,
  "description": "Ample Next.js Starter",
  "version": "0.1.0",
  "dependencies": {
    "@storybook/addon-a11y": "^5.3.8",
    "@storybook/addon-docs": "^5.3.8",
    "@storybook/addon-essentials": "^5.3.8",
    "@storybook/addon-jest": "^5.3.17",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "next",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.1",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.2",
    "next": "9.3.2",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "postcss-responsive-type": "^1.0.0",
    "prettier": "^2.0.2",
    "prettier-stylelint": "^0.4.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-formatter-pretty": "^2.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.0"
  },
  "keywords": [
    "ample",
    "next.js"
  ],
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "dev": "next dev",
    "develop": "next dev",
    "develop:debug": "DEBUG_MEDIA_QUERIES=true next dev",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint -c .eslintrc.js .",
    "lint:js:fix": "eslint -c .eslintrc.js . --fix",
    "lint:styles": "stylelint --config stylelint.config.js './**/*.scss'",
    "lint:styles:fix": "stylelint --config stylelint.config.js './**/*.scss' --fix",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "yarn run test:generate-output && build-storybook -c .storybook -o build/",
    "test": "jest ./src",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test:generate-output:watch": "yarn run test:generate-output --watch",
    "test:update": "jest ./src --updateSnapshot",
    "test:watch": "jest ./src --watch"
  },
  "browserslist": [
    "Edge >=13",
    "ie 11",
    "last 4 iOS major versions",
    "last 4 Chrome versions",
    "last 4 Firefox versions",
    "last 5 Android versions",
    "last 5 ChromeAndroid versions",
    "Safari >= 10"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ample/next-starter-ample"
  },
  "bugs": {
    "url": "https://github.com/ample/next-starter-ample/issues"
  }
}
